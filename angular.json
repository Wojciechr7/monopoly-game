{
  "version": 1,
  "projects": {
    "zulopoly": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/zulopoly",
      "sourceRoot": "apps/zulopoly/src",
      "prefix": "zulopoly",
      "architect": {
        "serve-with-services": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "ng serve api"
              },
              {
                "command": "ng serve zulopoly"
              }
            ],
            "parallel": true
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/zulopoly",
            "index": "apps/zulopoly/src/index.html",
            "main": "apps/zulopoly/src/main.ts",
            "polyfills": "apps/zulopoly/src/polyfills.ts",
            "tsConfig": "apps/zulopoly/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/zulopoly/src/favicon.ico",
              "apps/zulopoly/src/assets"
            ],
            "styles": [
              "apps/zulopoly/src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/nova-light/theme.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/zulopoly/src/environments/environment.ts",
                  "with": "apps/zulopoly/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "zulopoly:build",
            "proxyConfig": "apps/zulopoly/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "zulopoly:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "zulopoly:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/zulopoly/tsconfig.app.json",
              "apps/zulopoly/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/zulopoly/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/zulopoly/jest.config.js",
            "tsConfig": "apps/zulopoly/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/zulopoly/src/test-setup.ts"
          }
        }
      }
    },
    "zulopoly-e2e": {
      "root": "apps/zulopoly-e2e",
      "sourceRoot": "apps/zulopoly-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/zulopoly-e2e/cypress.json",
            "tsConfig": "apps/zulopoly-e2e/tsconfig.e2e.json",
            "devServerTarget": "zulopoly:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "zulopoly:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/zulopoly-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/zulopoly-e2e/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/api-interfaces/tsconfig.lib.json",
              "libs/api-interfaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/api-interfaces/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "tsConfig": "libs/api-interfaces/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "base": {
      "projectType": "library",
      "root": "libs/base",
      "sourceRoot": "libs/base/src",
      "prefix": "zulopoly",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/base/tsconfig.lib.json",
              "libs/base/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/base/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/base/jest.config.js",
            "tsConfig": "libs/base/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/base/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "stylext": "scss"
        }
      }
    },
    "style": {
      "projectType": "library",
      "root": "libs/style",
      "sourceRoot": "libs/style/src",
      "prefix": "zulopoly",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/style/tsconfig.lib.json",
              "libs/style/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/style/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/style/jest.config.js",
            "tsConfig": "libs/style/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/style/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "zulopoly-fields": {
      "projectType": "library",
      "root": "libs/zulopoly-fields",
      "sourceRoot": "libs/zulopoly-fields/src",
      "prefix": "zulopoly",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/zulopoly-fields/tsconfig.lib.json",
              "libs/zulopoly-fields/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/zulopoly-fields/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/zulopoly-fields/jest.config.js",
            "tsConfig": "libs/zulopoly-fields/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/zulopoly-fields/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "cd8b2d9e-79bf-4640-acc2-012802baeaad"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "zulopoly"
}
